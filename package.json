{
  "name": "11tyat",
  "version": "1.0.0",
  "description": "A opioninated 11ty boilerplate",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf src/_site",
    "serve": "serve --no-clipboard --listen 8080 dist",
    "dev": "cross-env NODE_ENV=development run-s clean dev:*",
    "dev:snowpack": "snowpack dev",
    "build": "cross-env NODE_ENV=production run-s clean build:*",
    "build:snowpack": "snowpack build",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .js,.html src/",
    "format": "run-s format:*",
    "format:eslint": "eslint --ext .js,.html src/ --fix",
    "validate": "run-s build lint"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@snowpack/plugin-postcss": "^1.1.0",
    "@snowpack/plugin-run-script": "^2.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "snowpack": "^3.0.11",
    "tailwindcss": "^2.0.3"
  },
  "dependencies": {
    "alpinejs": "^2.8.0"
  }
}
